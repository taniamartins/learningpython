import random

#define function and then imbed the if statement
def flip_coin ():
    random_integer = random.randint(1, 2)
    if random_integer == 1 :
        side = 'heads'
    else:
        side = 'tails'
    return side

choice = input('heads or tails: ')
result = flip_coin()
print('The coin landed on {}'.format(result))